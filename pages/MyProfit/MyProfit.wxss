/* pages/MyProfit/MyProfit.wxss */
page{
  background: #4E89D2;
}
.contain{
  padding: 30rpx;
}
.title{
  padding: 30rpx;
  background: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 32rpx;
  border-radius: 10rpx;
}
.title view:last-child{
  color: #4E89D2
}
.modules{
  background: #fff;
  padding: 30rpx;
  font-size: 32rpx;
  border-radius: 10rpx;
  margin-top: 20rpx;
}
.icon-wo{
  color: #4E89D2;
  font-size: 36rpx;
}
.icon-qian{
  color: #FF9800;
  font-size: 40rpx;
}
.content{
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px dashed #ccc;
}
.titleColor{
  color: #9F9F9F;
}
.modules_left{
  text-align: center
}
.contenter{
  padding: 10rpx 0;
  display: flex;
  align-items: center;
  justify-content: center;
}
.modules_btm{
  display: flex;
  padding-top: 20rpx;
  color: #9F9F9F;
}
.modules_btm text{
  color: #333
}
.modules_btm view:last-child{
  padding-left: 30rpx;
}